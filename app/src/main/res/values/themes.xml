<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.PhysioDate" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#00C7AA</item>
        <item name="colorPrimaryVariant">@color/purple_l</item>
        <item name="colorOnPrimary">@color/white</item>
        <item name="colorPrimary">@color/teal</item>
        <item name="colorPrimaryVariant">@color/teal_l</item>
        <item name="colorOnPrimary">@color/background_white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/blue</item>
        <item name="colorSecondaryVariant">@color/blue_l</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">#003700B3</item>
        <item name="android:statusBarColor">#00C7AA</item>
        <!-- Customize your theme here. -->

    </style>
    <style name="Theme.PhysioDate.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>
    <style name="Theme.MyApplication.AppBarOverlay"
        parent="ThemeOverlay.AppCompat.Dark.ActionBar" />
